{
  "required_status_checks": {
    "strict": true,
    "contexts": [
      "Lint/ESLint (TypeScript/JavaScript)",
      "Lint/Prettier (Code Formatting)",
      "Lint/Black (Python Formatting)",
      "Test/Backend Tests (Jest)",
      "Test/AI Service Tests (Pytest)",
      "Test/Frontend Tests (Vitest)",
      "Build Docker Images/Build Summary",
      "Build Docker Images/Detect Service Changes",
      "Test/Coverage Report"
    ]
  },
  "required_pull_request_reviews": {
    "dismiss_stale_reviews": true,
    "require_code_owner_reviews": false,
    "require_last_push_approval": false,
    "required_approving_review_count": 1
  },
  "enforce_admins": true,
  "required_linear_history": true,
  "allow_force_pushes": false,
  "allow_deletions": false,
  "restrictions": null
}
